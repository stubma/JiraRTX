<?xml version="1.0" encoding="UTF-8"?>

<atlassian-plugin key="luma.rtxnotifier-tests" name="rtxnotifier" plugins-version="2">
  <plugin-info>
    <description>This is the luma:rtxnotifier plugin for Atlassian JIRA.</description>
    <version>1.0.0</version>
    <vendor name="Example Company" url="http://www.example.com/"/>
  </plugin-info>
  <!-- from our base plugin -->
  <component-import key="myComponent" interface="luma.jira.plugin.rtxnotifier.MyPluginComponent"/>
  <!-- from the product container -->
  <component-import key="applicationProperties" interface="com.atlassian.sal.api.ApplicationProperties"/>
  <component key="my-component-wired-test" class="it.luma.jira.plugin.rtxnotifier.MyComponentWiredTest" public="true">
    <interface>it.luma.jira.plugin.rtxnotifier.MyComponentWiredTest</interface>
    <service-properties>
      <entry key="inProductTest" value="true"/>
    </service-properties>
  </component>
</atlassian-plugin>
